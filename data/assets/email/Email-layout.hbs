    <table class="layout-table" key="3" align="center" style="table-layout:fixed;height:100%;width:100%;" cellspacing="0" cellpadding="0">
        <colgroup class="displayNone" key="1">
            <col width="50%" key="0">
            <col width="{{#if width}}{{round (divide width 6)}}{{else}}107{{/if}}px" key="1">
            <col width="{{#if width}}{{round (divide width 6)}}{{else}}107{{/if}}px" key="2">
            <col width="{{#if width}}{{round (divide width 6)}}{{else}}107{{/if}}px" key="3">
