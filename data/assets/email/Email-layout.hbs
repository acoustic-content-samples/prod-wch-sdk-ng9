            <col width="50%" key="7">
        </colgroup>
        <tbody key="2"{{#if rows.0.cells}} class="source target"{{/if}}>
        {{#each rows}}
            <tr class="layout-grid-row displayBlock"  style="height:100%;{{>styles}}{{>trBackgroundStyle}}"{{#if key}} key="{{key}}"{{/if}} data-content-item-id="{{@root.$metadata.id}}" data-wch-editable="elements.rows.values[{{@index}}]" data-wch-selectable="{{@root.$metadata.id}}#rows.values[{{@index}}]">
                <td class="layout-grid-section-cell displayNone" key="before">{{!-- Zero column !--}}</td>
                {{#each cells}}
                    <td class="layout-grid-cell displayBlock target{{#if content}} source{{/if}}" key="{{@index}}" colSpan="{{span}}" valign="top" style="{{#if content}}height:100%;{{/if}}vertical-align:top;{{>contentAreaPadding row=(plus @../index 1) num-rows=../../rows.length cell=(plus @index 1) num-cells=../cells.length}}{{#if (and @root.contentAreaBackgroundColor.colorCode (not (or ../background.backgroundColor.colorCode ../background.includeBackgroundImage.value))) }}background-color:{{@root.contentAreaBackgroundColor.colorCode}};{{/if}}" data-content-item-id="{{@root.$metadata.id}}" data-wch-editable="elements.rows.values[{{@../index}}].cells.values[{{@index}}].content.values">
                        {{#each content}}
                            <div class="layout-grid-cell-content" key="{{ $metadata.id}}" style="{{#equalsLength ../content/values 1}}height:100%;{{/equalsLength}}overflow-wrap:anywhere;overflow:hidden;" data-content-item-id="{{@root.$metadata.id}}" data-wch-editable="elements.rows.values[{{@../../index}}].cells.values[{{@../index}}].content.values[{{@index}}]" data-wch-selectable="{{ $metadata.id}}">
                                {{{ $markup }}}
                            </div>
                        {{/each}}
                    </td>
                {{/each}}
                <td class="layout-grid-section-cell displayNone" key="after">{{!-- Seventh column !--}}</td>
            </tr>
        {{else}}
            <tr class="layout-grid-row-empty" key="empty-row" data-content-item-id="{{ $metadata.id}}" data-wch-editable="{{ $metadata.accessor }}.rows.values[0]">
                <td class="layout-grid-section-cell" key="0">{{!-- Zero column !--}}</td>
                <td class="layout-grid-cell-empty" key="empty-cell" colSpan="6"></td>
                <td class="layout-grid-section-cell" key="7">{{!-- Seventh column !--}}</td>
            </tr>
        {{/each}}
