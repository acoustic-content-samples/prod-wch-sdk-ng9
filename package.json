{
  "name": "prod-wch-sdk-ng9",
  "private": true,
  "bugs": {
    "web": "https://developer.ibm.com/answers/smartspace/wch/index.html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aipoweredmarketer/prod-wch-sdk-ng9.git"
  },
  "license": "MIT",
  "author": "Carsten Leue <carsten.leue@de.ibm.com>",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "prebuild": "ts-node scripts/prebuild.ts",
    "build": "wsrun --stages --fast-exit --collect-logs --report build:packagr ",
    "postbuild": "npm-run-all build:api build:doc build:post",
    "build:post": "ts-node scripts/postbuild.ts",
    "build:api": "wsrun --exclude-missing -c build:api",
    "build:doc": "wsrun --exclude-missing -c build:doc",
    "build:serial": "wsrun --serial build:packagr",
    "config:artifactory": "yarn config set registry https://na.artifactory.swg-devops.com/artifactory/api/npm/wce-wch-npm-virtual/ && yarn config get registry",
    "config:verdaccio": "yarn config set registry http://wats-repob.devlab.ibm.com && yarn config get registry",
    "depcheck": "yarn workspaces run exitzero depcheck -o depcheck/depcheck.txt",
    "from:prod-publishing-content-editor": "sync-files --no-notify-update ../prod-publishing-content-editor/dist ./node_modules/prod-publishing-content-editor",
    "lint": "yarn workspaces run exitzero tslint -p . -o lint/lint.txt -t codeFrame",
    "patch": "npm-run-all from:prod-publishing-content-editor",
    "publish": "npm-run-all publish:verdaccio",
    "publish:artifactory": "npm-run-all config:artifactory publish:run",
    "publish:global": "ts-node scripts/publish.ts",
    "publish:run": "yarn workspaces run publish dist --non-interactive",
    "publish:verdaccio": "npm-run-all config:verdaccio publish:run",
    "pull": "wchtools pull -A --dir temp --verbose",
    "push": "wchtools push -A --dir data --verbose",
    "test": "wsrun test --passWithNoTests --json --outputFile test.result.json"
  },
