{
  "name": "@acoustic-content-sdk/react-utils",
  "version": "9.0.0",
  "description": "Collection of utility methods to efficiently create react components and modules.",
  "keywords": [
    "acoustic",
    "acoustic-content",
    "react",
    "sdk"
  ],
  "bugs": {
    "web": "https://developer.ibm.com/answers/smartspace/wch/index.html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aipoweredmarketer/prod-wch-sdk-ng9.git"
  },
  "license": "MIT",
  "author": "Carsten Leue <carsten.leue@de.ibm.com>",
  "sideEffects": false,
  "main": "dist/bundles/acoustic-content-sdk-react-utils.umd.js",
  "module": "dist/fesm5/acoustic-content-sdk-react-utils.js",
  "typings": "dist/acoustic-content-sdk-react-utils.d.ts",
  "scripts": {
    "build:api": "api-extractor run --local",
    "build:doc": "api-documenter markdown -i temp",
    "build:packagr": "ng-packagr -p package.json -c tsconfig.json",
    "test": "jest"
  },
  "dependencies": {
    "@acoustic-content-sdk/api": "9.0.0",
    "@acoustic-content-sdk/edit-api": "9.0.0",
    "@acoustic-content-sdk/utils": "9.0.0"
  },
  "peerDependencies": {
    "react": "^16",
    "rfc6902": "^3",
    "rxjs": "^6"
  },
  "es2015": "dist/fesm2015/acoustic-content-sdk-react-utils.js",
  "esm2015": "dist/esm2015/acoustic-content-sdk-react-utils.js",
  "esm5": "dist/esm5/acoustic-content-sdk-react-utils.js",
  "fesm2015": "dist/fesm2015/acoustic-content-sdk-react-utils.js",
  "fesm5": "dist/fesm5/acoustic-content-sdk-react-utils.js",
  "metadata": "dist/acoustic-content-sdk-react-utils.metadata.json",
  "ngPackage": {
    "lib": {
      "umdModuleIds": {
        "@acoustic-content-sdk/api": "acoustic-content-sdk.api",
        "@acoustic-content-sdk/edit-api": "acoustic-content-sdk.edit-api",
        "@acoustic-content-sdk/utils": "acoustic-content-sdk.utils",
        "react": "React",
        "rfc6902": "Rfc6902"
      }
    },
    "whitelistedNonPeerDependencies": [
      "@acoustic-content-sdk/.*",
      "loose-envify"
    ]
  },
  "tsdoc": {
    "tsdocFlavor": "AEDoc"
  }
