{
  "name": "@acoustic-content-sdk/ng-app",
  "version": "9.0.0",
  "description": "Container module used to pull in consistent dependencies for ng-app-live, ng-app-preview and ng-app-base.",
  "keywords": [
    "acoustic",
    "acoustic-content",
    "angular",
    "sdk"
  ],
  "bugs": {
    "web": "https://developer.ibm.com/answers/smartspace/wch/index.html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aipoweredmarketer/prod-wch-sdk-ng9.git"
  },
  "license": "MIT",
  "author": "Carsten Leue <carsten.leue@de.ibm.com>",
  "sideEffects": false,
  "main": "dist/bundles/acoustic-content-sdk-ng-app.umd.js",
  "module": "dist/fesm5/acoustic-content-sdk-ng-app.js",
  "typings": "dist/acoustic-content-sdk-ng-app.d.ts",
  "scripts": {
    "build:api": "api-extractor run --local",
    "build:doc": "api-documenter markdown -i temp",
    "build:packagr": "ng-packagr -p package.json -c tsconfig.json",
    "test": "jest"
  },
  "dependencies": {
    "@acoustic-content-sdk/ng-app-base": "9.0.0",
    "@acoustic-content-sdk/ng-app-live": "9.0.0",
    "@acoustic-content-sdk/ng-app-preview": "9.0.0"
  },
  "peerDependencies": {},
  "es2015": "dist/fesm2015/acoustic-content-sdk-ng-app.js",
  "esm2015": "dist/esm2015/acoustic-content-sdk-ng-app.js",
  "esm5": "dist/esm5/acoustic-content-sdk-ng-app.js",
  "fesm2015": "dist/fesm2015/acoustic-content-sdk-ng-app.js",
  "fesm5": "dist/fesm5/acoustic-content-sdk-ng-app.js",
  "metadata": "dist/acoustic-content-sdk-ng-app.metadata.json",
  "ngPackage": {
    "lib": {
      "umdModuleIds": {
        "@acoustic-content-sdk/ng-app-base": "acoustic-content-sdk.ng-app-base",
        "@acoustic-content-sdk/ng-app-live": "acoustic-content-sdk.ng-app-live",
        "@acoustic-content-sdk/ng-app-preview": "acoustic-content-sdk.ng-app-preview"
      }
    },
    "whitelistedNonPeerDependencies": [
      "@acoustic-content-sdk/.*"
    ]
  },
  "tsdoc": {
    "tsdocFlavor": "AEDoc"
  }
}