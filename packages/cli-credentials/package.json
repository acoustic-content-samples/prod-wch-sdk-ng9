  },
  "license": "MIT",
  "author": "Carsten Leue <carsten.leue@de.ibm.com>",
  "sideEffects": false,
  "main": "dist/bundles/acoustic-content-sdk-cli-credentials.umd.js",
  "module": "dist/fesm5/acoustic-content-sdk-cli-credentials.js",
  "typings": "dist/acoustic-content-sdk-cli-credentials.d.ts",
  "scripts": {
    "build:packagr": "ng-packagr -p package.json -c tsconfig.json",
    "test": "jest --runInBand"
  },
  "dependencies": {
    "@acoustic-content-sdk/api": "9.0.0"
  },
  "peerDependencies": {
    "email-validator": "^2",
