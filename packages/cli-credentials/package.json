  },
  "es2015": "dist/fesm2015/acoustic-content-sdk-cli-credentials.js",
  "esm2015": "dist/esm2015/acoustic-content-sdk-cli-credentials.js",
  "esm5": "dist/esm5/acoustic-content-sdk-cli-credentials.js",
  "fesm2015": "dist/fesm2015/acoustic-content-sdk-cli-credentials.js",
  "fesm5": "dist/fesm5/acoustic-content-sdk-cli-credentials.js",
  "metadata": "dist/acoustic-content-sdk-cli-credentials.metadata.json",
  "ngPackage": {
    "lib": {
      "umdModuleIds": {
        "@acoustic-content-sdk/api": "acoustic-content-sdk.api",
        "email-validator": "EmailValidator",
        "sshpk": "Sshpk",
        "valid-url": "ValidUrl"
      }
    },
    "whitelistedNonPeerDependencies": [
      ".*"
    ]
  },
  "tsdoc": {
    "tsdocFlavor": "AEDoc"
