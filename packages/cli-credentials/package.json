{
  "name": "@acoustic-content-sdk/cli-credentials",
  "version": "9.0.0",
  "description": "Helper library to store and retrieve credentials for the CLI.",
  "keywords": [
    "acoustic",
    "acoustic-content",
    "cli",
    "sdk"
  ],
  "bugs": {
    "web": "https://developer.ibm.com/answers/smartspace/wch/index.html"
  },
  "repository": {
    "type": "git",
