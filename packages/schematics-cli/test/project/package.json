{
  "name": "project",
  "config": {
    "name": "project",
    "data": "./data",
    "root": "/",
    "src": "./src"
  },
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@acoustic-content-sdk/api": "^7.2.348",
    "@acoustic-content-sdk/app": "^7.2.348",
    "@acoustic-content-sdk/ng": "^7.2.348",
    "@acoustic-content-sdk/ng-edit": "^7.2.348",
    "@acoustic-content-sdk/utils": "^7.2.348",
    "core-js": "^2.5.4",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.1",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@acoustic-content-sdk/cli": "^7.2.348",
    "@acoustic-content-sdk/schematics": "^7.2.348",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "cpx": "^1.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "npm-run-all": "^4.1.3",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  },
  "private": true,
  "scripts": {
    "build": "ng build",
    "build:development": "acoustic-content-sdk-cli build development --relative",
    "build:production": "acoustic-content-sdk-cli build production --skipSourcemaps --relative",
    "deploy": "npm-run-all build:production package:production deploy:push show:url",
    "deploy:purge": "acoustic-content-sdk-cli application purge",
    "deploy:push": "acoustic-content-sdk-cli application push --verbose --override --aggregated",
    "e2e": "ng e2e",
    "gen:canonicalize": "acoustic-content-sdk-cli canonicalize assets",
    "gen:components": "acoustic-content-sdk-cli create components --scss  --flat --modules",
    "gen:layouts": "acoustic-content-sdk-cli create layouts",
    "gen:pagetype": "acoustic-content-sdk-cli create pagetype --scss --type",
    "gen:pull": "cpx ./data/.wchtoolsoptions.json ./tmp/ && acoustic-content-sdk-cli application pull --data ./tmp/",
    "lint": "ng lint",
    "ng": "ng",
    "package:addBaseUrl": "acoustic-content-sdk-cli add base --relative",
    "package:addEsiPolyfill": "acoustic-content-sdk-cli add esi-polyfill",
    "package:addManifest": "acoustic-content-sdk-cli add webappmanifest --relative",
    "package:addPrefetch": "acoustic-content-sdk-cli add prefetch --relative",
    "package:compress": "acoustic-content-sdk-cli compress assets",
    "package:copyResources": "acoustic-content-sdk-cli copy build",
    "package:createSite": "acoustic-content-sdk-cli create site",
    "package:production": "npm-run-all package:copyResources package:addPrefetch package:addBaseUrl package:addManifest package:addEsiPolyfill package:createSite",
    "run:wchtools": "acoustic-content-sdk-cli run wchtools",
    "show:types": "acoustic-content-sdk-cli show types",
    "show:url": "acoustic-content-sdk-cli show url",
    "start": "ng serve",
    "test": "ng test"
  },
  "version": "0.0.0",
  "wchtools-dependencies": [
    "./data"
  ]
}