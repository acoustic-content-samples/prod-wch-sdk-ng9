{
  "name": "@acoustic-content-sdk/cli-utils",
  "version": "9.0.0",
  "description": "Helper library for rx helpers around node APIs.",
  "keywords": [
    "acoustic",
    "acoustic-content",
    "sdk"
  ],
  "bugs": {
    "web": "https://developer.ibm.com/answers/smartspace/wch/index.html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aipoweredmarketer/prod-wch-sdk-ng9.git"
  },
  "license": "MIT",
  "author": "Carsten Leue <carsten.leue@de.ibm.com>",
  "sideEffects": false,
  "main": "dist/bundles/acoustic-content-sdk-cli-utils.umd.js",
  "module": "dist/fesm5/acoustic-content-sdk-cli-utils.js",
  "typings": "dist/acoustic-content-sdk-cli-utils.d.ts",
  "scripts": {
    "build:packagr": "ng-packagr -p package.json -c tsconfig.json",
    "test": "jest"
  },
  "dependencies": {
    "@acoustic-content-sdk/api": "9.0.0",
    "@acoustic-content-sdk/cli-credentials": "9.0.0",
    "@acoustic-content-sdk/rx-utils": "9.0.0",
    "@acoustic-content-sdk/utils": "9.0.0"
  },
  "peerDependencies": {
    "mime": "^2",
    "rxjs": "^6",
    "semver": "^7",
    "typescript": "^3",
    "uuid": "^3",
    "verror": "^1"
  },
  "es2015": "dist/fesm2015/acoustic-content-sdk-cli-utils.js",
  "esm2015": "dist/esm2015/acoustic-content-sdk-cli-utils.js",
  "esm5": "dist/esm5/acoustic-content-sdk-cli-utils.js",
  "fesm2015": "dist/fesm2015/acoustic-content-sdk-cli-utils.js",
  "fesm5": "dist/fesm5/acoustic-content-sdk-cli-utils.js",
  "metadata": "dist/acoustic-content-sdk-cli-utils.metadata.json",
  "ngPackage": {
    "lib": {
      "umdModuleIds": {
        "@acoustic-content-sdk/api": "acoustic-content-sdk.api",
        "@acoustic-content-sdk/cli-credentials": "acoustic-content-sdk.cli-credentials",
        "@acoustic-content-sdk/rx-utils": "acoustic-content-sdk.rx-utils",
        "@acoustic-content-sdk/utils": "acoustic-content-sdk.utils",
        "mime": "Mime",
        "semver": "Semver",
        "typescript": "Typescript",
        "uuid": "Uuid",
        "verror": "Verror"
      }
    },
    "whitelistedNonPeerDependencies": [
      "@acoustic-content-sdk/.*",
      "semver",
      "mime",
      "uuid",
      "verror"
    ]
  },
  "tsdoc": {
    "tsdocFlavor": "AEDoc"
  }
}
