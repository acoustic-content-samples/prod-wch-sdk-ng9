{
  "name": "proto-sites-next-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "ng build",
    "e2e": "ng e2e",
    "from:api": "cpx --verbose --preserve \"../prod-wch-sdk-ng9/packages/api/dist/**/*\" ./node_modules/@ibm-wch-sdk/api",
    "from:cross-frame-redux": "cpx --verbose --preserve \"../prod-wch-sdk-ng9/packages/cross-frame-redux/dist/**/*\" ./node_modules/@ibm-wch-sdk/cross-frame-redux",
    "from:ng": "cpx --verbose --preserve \"../prod-wch-sdk-ng9/packages/ng/dist/**/*\" ./node_modules/@ibm-wch-sdk/ng",
    "from:ng-api": "cpx --verbose --preserve \"../prod-wch-sdk-ng9/packages/ng-api/dist/**/*\" ./node_modules/@ibm-wch-sdk/ng-api",
    "from:ng-logger": "cpx --verbose --preserve \"../prod-wch-sdk-ng9/packages/ng-logger/dist/**/*\" ./node_modules/@ibm-wch-sdk/ng-logger",
    "from:ng-redux": "cpx --verbose --preserve \"../prod-wch-sdk-ng9/packages/ng-redux/dist/**/*\" ./node_modules/@ibm-wch-sdk/ng-redux",
    "from:ng-utils": "cpx --verbose --preserve \"../prod-wch-sdk-ng9/packages/ng-utils/dist/**/*\" ./node_modules/@ibm-wch-sdk/ng-utils",
    "from:prod-publishing-content-editor": "cpx --verbose --preserve \"../prod-publishing-content-editor/dist/**/*\" ./node_modules/prod-publishing-content-editor",
    "from:redux-store": "cpx --verbose --preserve \"../prod-wch-sdk-ng9/packages/redux-store/dist/**/*\" ./node_modules/@ibm-wch-sdk/redux-store",
    "from:schematics-cli": "cpx --verbose --preserve \"../prod-wch-sdk-ng9/packages/schematics-cli/dist/**/*\" ./node_modules/@ibm-wch-sdk/schematics",
    "from:utils": "cpx --verbose --preserve \"../prod-wch-sdk-ng9/packages/utils/dist/**/*\" ./node_modules/@ibm-wch-sdk/utils",
    "lint": "ng lint",
    "ng": "ng",
    "ng-high-memory": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng",
    "patch": "npm-run-all --parallel from:ng-redux from:prod-publishing-content-editor from:redux-store from:ng from:cross-frame-redux from:api from:utils from:ng-logger from:ng-utils from:ng-api from:schematics-cli",
    "pull": "wchtools pull --dir data -A -v",
    "push": "wchtools push --dir data -A -v",
    "start": "ng serve",
    "test": "ng test"
  },
  "dependencies": {
