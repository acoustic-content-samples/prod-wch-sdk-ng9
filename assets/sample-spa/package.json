    "@ibm-wch-sdk/ng-redux": "^8",
    "@ibm-wch-sdk/ng-utils": "^8",
    "@ibm-wch-sdk/ngx-drag-and-drop-lists": "^8",
    "@ibm-wch-sdk/redux-store": "^8",
    "@ibm-wch-sdk/utils": "^8",
    "@prod-publishing-react/authoring-blocks": "^8.0.12",
    "@prod-publishing-react/block-settings": "^8.0.12",
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "carbon-components": "^10",
    "carbon-components-react": "^7",
    "codemirror": "^5.48.2",
    "core-js": "^2.5.4",
    "dragula": "^3.7.2",
    "fs": "^0.0.2",
    "immer": "^3.1.3",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "prod-publishing-content-editor": "^2.0.917",
    "prod-publishing-handlebars-helpers": "^1.0.63",
    "react": "^16.8.6",
    "react-beautiful-dnd": "^11.0.5",
    "react-codemirror2": "^6.0.0",
    "react-color": "^2.17.3",
    "react-dom": "^16.8.6",
    "react-draggable": "^3.3.0",
    "redux": "^4.0.4",
    "redux-actions": "^2.6.5",
    "redux-devtools-extension": "^2.13.8",
