{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-sonarts",
    "rxjs-tslint-rules",
    "tslint-config-prettier"
  ],
  "rules": {
    "interface-name": [true, "never-prefix"],
    "member-access": false,
    "ordered-imports": false,
    "object-literal-sort-keys": false
  }
}
